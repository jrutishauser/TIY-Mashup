{
  "manifest_version": 2,

  "name": "SusannaBot",
  "description": "SusannaBot here to listen",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/susanna-bot.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
    "icons": {
        "16": "img/susanna-bot.png",
        "48": "img/susanna-bot.png",
        "128": "img/susanna-bot.png"
    }
}
